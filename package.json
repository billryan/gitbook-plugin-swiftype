{
    "name": "gitbook-plugin-swiftype",
    "description": "Swiftype for your gitbook with custom search engine",
    "version": "0.1.1",
    "engines": {
        "gitbook": ">=3.0.0-pre.0"
    },
    "homepage": "https://github.com/billryan/gitbook-plugin-swiftype",
    "repository": {
        "type": "git",
        "url": "https://github.com/billryan/gitbook-plugin-swiftype.git"
    },
    "license": "Apache 2",
    "bugs": {
        "url": "https://github.com/billryan/gitbook-plugin-swiftype/issues"
    },
    "gitbook": {
        "properties": {
            "token": {
                "type": "string",
                "description": "Swiftype token key",
                "required": true
            },
            "version": {
                "type": "string",
                "description": "Swiftype version",
                "default": "2.0.0"
            }
        }
    }
}
